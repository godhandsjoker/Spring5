<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="user" class="spring5.User"></bean>
    <bean id="book" class="spring5.Book">
        <property name="author" value="Java"></property>
        <property name="name" value="chending"></property>
    </bean>

    <bean id="order" class="spring5.Order">
        <constructor-arg index="0" value="aa"/>
        <constructor-arg index="1" value="bb"/>
    </bean>

    <bean id="book1" class="spring5.Book" p:name="cc" p:author="dd"></bean>
</beans>